<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">
    <display-name>qrcode</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>QrCodeQueryServlet</servlet-name>
        <servlet-class>com.tmind.qrcode.servlet.QrCodeQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QrCodeQueryServlet</servlet-name>
        <url-pattern>/qrCodeQueryServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CommonTemplateQueryServlet</servlet-name>
        <servlet-class>com.tmind.qrcode.servlet.CommonTemplateQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CommonTemplateQueryServlet</servlet-name>
        <url-pattern>/commonTemplateQueryServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MakeOrderViaEmailServlet</servlet-name>
        <servlet-class>com.tmind.qrcode.servlet.MakeOrderViaEmailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MakeOrderViaEmailServlet</servlet-name>
        <url-pattern>/m/r/y/makeOrderViaEmailServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WebQueryServlet</servlet-name>
        <servlet-class>com.tmind.qrcode.servlet.WebQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WebQueryServlet</servlet-name>
        <url-pattern>/webQueryServlet</url-pattern>
    </servlet-mapping>

</web-app>